(defnet test-webp (func)
        (deflocal i)

        (lmatch remove func "webp-")
        (for i in (test-webp-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_webp_" (dash->underscore func) "(") "))")
        (flag-true "webp") )

(defun test-webp-table ()
        [ [ "save"              2 3 ]
        ] )


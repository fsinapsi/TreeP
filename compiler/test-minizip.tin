(defnet test-minizip (func)
        (deflocal i)

        (lmatch remove func "minizip-")
        (for i in (test-minizip-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_minizip_" (dash->underscore func) "(") "))")
        (flag-true "minizip") )

(defun test-minizip-table ()
        [ [ "zip"       3 4 ]
          [ "unzip"     2 2 ]
        ] )


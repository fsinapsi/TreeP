(defnet test-avcodec (func)
        (deflocal i)

        (lmatch remove func "av-")
        (for i in (test-avcodec-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_av_" (dash->underscore func) "(") "))")
        (flag-true "avcodec") )

(defun test-avcodec-table ()
        [ [ "sws-scale" 3 3 ]
          [ "read-frame" 2 3 ]
          [ "skip-frame" 1 2 ]
          [ "seek-frame" 2 2 ]
        ] )


(defnet test-dbf (func)
        (deflocal i)

        (lmatch remove func "dbf-")
        (for i in (test-dbf-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_dbf_" (dash->underscore func) "(") "))")
        (flag-true "dbf") )

(defun test-dbf-table ()
        [ [ "copy-record"       2 2 ]
        ] )


(defnet test-microhttpd (func)
        (deflocal i)

        (lmatch remove func "mhd-")
        (for i in (test-microhttpd-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_mhd_" (dash->underscore func) "(") "))")
        (flag-true "microhttpd") )

(defun test-microhttpd-table ()
        [ [ "run-wait"          1 2 ]
          [ "stop-daemon"       1 1 ]
        ] )


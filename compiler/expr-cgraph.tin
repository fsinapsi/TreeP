(defnet expr-cgraph (func)
        (deflocal i)

        (lmatch remove func "ag-")
        (for i in (expr-cgraph-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-basic <i 1> <i 2> (+ "trp_ag_" (dash->underscore func) "(") ")")
        (flag-true "cgraph") )

(defun expr-cgraph-table ()
        [ [ "dot2pix"                   1 1 ]
          [ "dot2pix-transparent"       1 1 ]
        ] )


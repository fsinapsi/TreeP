(defnet test-magic (func)
        (deflocal i)

        (lmatch remove func "magic-")
        (for i in (test-magic-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_magic_" (dash->underscore func) "(") "))")
        (flag-true "magic") )

(defun test-magic-table ()
        [ [ "reinit"                    1 1 ]
        ] )


(defnet test-sdl (func)
        (deflocal i)

        (lmatch remove func "sdl-")
        (for i in (test-sdl-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_sdl_" (dash->underscore func) "(") "))")
        (flag-true "sdl") )

(defun test-sdl-table ()
        [ [ "playwav"           1 2 ]
          [ "playwav-memory"    1 2 ]
        ] )


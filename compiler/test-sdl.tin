(defnet test-sdl (func)
        (deflocal i)

        (lmatch remove func "sdl-")
        (for i in (test-sdl-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_sdl_" (dash->underscore func) "(") "))")
        (flag-true "sdl") )

(defun test-sdl-table ()
        [ [ "enable-screen-saver"               0 0 ]
          [ "disable-screen-saver"              0 0 ]
          [ "pause-audio-stream-device"         1 1 ]
          [ "resume-audio-stream-device"        1 1 ]
          [ "set-audio-stream-gain"             2 2 ]
          [ "flush-audio-stream"                1 1 ]
          [ "clear-audio-stream"                1 1 ]
          [ "put-audio-stream-data"             2 3 ]
          ;[ "audio-play"                        5 6 ]
          [ "set-window-position"               3 3 ]
          [ "set-window-size"                   3 3 ]
          [ "set-window-icon"                   2 2 ]
          [ "set-window-fullscreen"             2 2 ]
          [ "set-window-aspect-ratio"           3 3 ]
          [ "set-render-draw-color"             2 2 ]
          [ "render-clear"                      1 1 ]
          [ "render-texture"                    2 2 ]
          [ "render-present"                    1 1 ]
          [ "update-texture"                    2 2 ]
          [ "poll-event"                        1 1 ]
          [ "delay"                             1 1 ]
          [ "delay-ns"                          1 1 ]
          [ "delay-precise"                     1 1 ]
          [ "bps24-to-bps32"                    2 2 ]
        ] )


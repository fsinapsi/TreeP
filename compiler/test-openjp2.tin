(defnet test-openjp2 (func)
        (deflocal i)

        (lmatch remove func "openjp2-")
        (for i in (test-openjp2-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_openjp2_" (dash->underscore func) "(") "))")
        (flag-true "openjp2") )

(defun test-openjp2-table ()
        [ [ "save"              2 3 ]
        ] )


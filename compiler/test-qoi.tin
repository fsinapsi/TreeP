(defnet test-qoi (func)
        (deflocal i)

        (lmatch remove func "qoi-")
        (for i in (test-qoi-table) do
                until (= <i 0> func) )
        (= <i 0> func)
        (exprseq-ext <i 1> <i 2> (+ "  if(trp_qoi_" (dash->underscore func) "(") "))")
        (flag-true "qoi") )

(defun test-qoi-table ()
        [ [ "save"              2 2 ]
        ] )

